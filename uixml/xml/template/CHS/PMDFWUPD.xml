    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="P1" ID="P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MAIN">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.0.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>50</MAXLENGTH>
                <NAME>FILE_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>100</MAXLENGTH>
                <NAME>FILE_PATH</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.2.LBL}</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="F" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.2.OPTION.0.#text}</OPTION>
                <OPTION VALUE="P" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.2.OPTION.1.#text}</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>UPLOAD_TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.3.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnSubmitParams()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_UPLOAD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_FW_DIR_UPD</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
