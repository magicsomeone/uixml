      <BUTTON_EVENT>fnParticipantSummary()</BUTTON_EVENT>
      <BUTTON_LABEL>Participant Drawdown Summary</BUTTON_LABEL>
      <BUTTON_NAME>BTN_PARTI_SUM</BUTTON_NAME>
    </BUTTON>
    <BUTTONS_PER_ROW>0</BUTTONS_PER_ROW>
  </SUMBUTTONS>
  <BLOCK TABPAGE="All" TYPE="Single Entry" SCREEN="SUMMARY" VIEW="Single Entry">
    <FIELD TABPAGE="All">
      <SRC>Images/Exit2.gif</SRC>
      <AUTHSCRN></AUTHSCRN>
      <SIZE></SIZE>
      <HIDDEN>0</HIDDEN>
      <VALUE>Exit</VALUE>
      <CHECKED></CHECKED>
      <READ_ONLY></READ_ONLY>
      <EVENT>
        <FUNCTION>fnExit()</FUNCTION>
        <NAME>onClick</NAME>
      </EVENT>
      <NAME>BTN_EXIT</NAME>
      <DISABLED></DISABLED>
      <SUMMARY></SUMMARY>
      <ABS_POS></ABS_POS>
      <LABEL></LABEL>
      <ID>BTN_EXIT</ID>
      <SHOWIN>All</SHOWIN>
      <TYPE>BUTTON</TYPE>
      <TABINDEX></TABINDEX>
    </FIELD>
    <REQUIRED></REQUIRED>
    <WIDTH></WIDTH>
    <OKCANCEL>0</OKCANCEL>
    <HEIGHT></HEIGHT>
    <DATAPAGESIZE></DATAPAGESIZE>
    <ABS_POS></ABS_POS>
    <LABEL></LABEL>
    <ID>BLK_SUMMARY_EXIT</ID>
  </BLOCK>
