      <OPTION VALUE="A" SELECTED="0">${FORM.SUMBLOCK.0.FIELD.1.OPTION.1.#text}</OPTION>
      <OPTION VALUE="H" SELECTED="0">${FORM.SUMBLOCK.0.FIELD.1.OPTION.2.#text}</OPTION>
