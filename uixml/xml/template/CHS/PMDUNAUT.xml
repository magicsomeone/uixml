<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SUMMARY CRITERIA_SRCH="N" POSITION="TEMPLATE" TITLE="${FORM.SUMMARY.-TITLE}" TMP_SCR_TYPE="M">
    <SUMMARY_BASE></SUMMARY_BASE>
    <SUMBUTTONS>
      <BUTTON_ROWS></BUTTON_ROWS>
      <BUTTON ID="1">
        <BUTTON_EVENT>fn_Reassign()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>REASSIGN</BUTTON_NAME>
      </BUTTON>
      <BUTTONS_PER_ROW></BUTTONS_PER_ROW>
    </SUMBUTTONS>
    <SUMMARY_DATA_BLK>BLK_UNAUTH_TXN_VIEW</SUMMARY_DATA_BLK>
    <SUMBLOCK TABPAGE="QUERY" TYPE="SE" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>DATE</TYPE>
        <MAXLENGTH>11</MAXLENGTH>
        <NAME>HOST_DATE</NAME>
      </FIELD>

    </SUMBLOCK>
    <SUMBLOCK TABPAGE="RESULT" TYPE="ME" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.0.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>8</MAXLENGTH>
        <READ_ONLY>-1</READ_ONLY>
        <NAME>HOST_CODE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.1.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>DATE</TYPE>
        <MAXLENGTH>11</MAXLENGTH>
        <NAME>HOST_DATE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.2.LBL}</LBL>
        <DTYPE>CHAR</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>8</MAXLENGTH>
        <NAME>FUNCTION_ID</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <MAX_DECIMALS></MAX_DECIMALS>
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.LBL}</LBL>
        <MAX_VAL></MAX_VAL>
        <DTYPE>NUMBER</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>22</MAXLENGTH>
        <MIN_VAL></MIN_VAL>
        <NAME>NUMBER_OF_TXN</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="5">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.4.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>12</MAXLENGTH>
        <NAME>MAKER_ID</NAME>
      </FIELD>

    </SUMBLOCK>
    <TYPE>S</TYPE>
  </SUMMARY>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>

      </TAB>
    </BODY>
  </SCREEN>
</FORM>