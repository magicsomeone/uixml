<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SUMMARY CRITERIA_SRCH="N" POSITION="TEMPLATE" TITLE="${FORM.SUMMARY.-TITLE}" TMP_SCR_TYPE="M">
    <SUMMARY_BASE></SUMMARY_BASE>
    <SUMBUTTONS>
      <BUTTON_ROWS></BUTTON_ROWS>
      <BUTTON ID="1">
        <BUTTON_EVENT>fnview()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_VIEW</BUTTON_NAME>
      </BUTTON>
      <BUTTONS_PER_ROW></BUTTONS_PER_ROW>
    </SUMBUTTONS>
    <SUMMARY_DATA_BLK>BLK_DB_OUT_RECALL_REQ_TXN</SUMMARY_DATA_BLK>
    <SUMBLOCK TABPAGE="QUERY" TYPE="SE" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.0.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>40</MAXLENGTH>
        <NAME>TXN_REF_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.1.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>7</MAXLENGTH>
        <NAME>BOOK_DATE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.2.LBL}</LBL>
        <OPTION VALUE="" SELECTED="-1"></OPTION>
        <OPTION VALUE="Y" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.0.FIELD.2.OPTION.1.#text}</OPTION>
        <OPTION VALUE="N" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.0.FIELD.2.OPTION.2.#text}</OPTION>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>SELECT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>GPI_ENABLED</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.3.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>20</MAXLENGTH>
        <NAME>LAST_RESPONSE_ACTION</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="5">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.4.LBL}</LBL>
        <OPTION VALUE="" SELECTED="-1"></OPTION>
        <OPTION VALUE="R" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.0.FIELD.4.OPTION.1.#text}</OPTION>
        <OPTION VALUE="A" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.0.FIELD.4.OPTION.2.#text}</OPTION>
        <OPTION VALUE="X" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.0.FIELD.4.OPTION.3.#text}</OPTION>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>SELECT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>RECALL_STATUS</NAME>
      </FIELD>

    </SUMBLOCK>
    <SUMBLOCK TABPAGE="RESULT" TYPE="ME" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.0.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>40</MAXLENGTH>
        <NAME>TXN_REF_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.1.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>7</MAXLENGTH>
        <NAME>BOOK_DATE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.2.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>3</MAXLENGTH>
        <NAME>BRANCH_CODE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.LBL}</LBL>
        <OPTION VALUE="" SELECTED="-1"></OPTION>
        <OPTION VALUE="Y" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.OPTION.1.#text}</OPTION>
        <OPTION VALUE="N" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.OPTION.2.#text}</OPTION>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>SELECT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>GPI_ENABLED</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="5">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.4.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>8</MAXLENGTH>
        <NAME>HOST_CODE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="6">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.5.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>20</MAXLENGTH>
        <NAME>LAST_RESPONSE_ACTION</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="7">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.6.LBL}</LBL>
        <OPTION VALUE="" SELECTED="-1"></OPTION>
        <OPTION VALUE="R" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.6.OPTION.1.#text}</OPTION>
        <OPTION VALUE="A" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.6.OPTION.2.#text}</OPTION>
        <OPTION VALUE="X" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.6.OPTION.3.#text}</OPTION>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>SELECT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>RECALL_STATUS</NAME>
      </FIELD>
      <LEGENDS>
        <LBL></LBL>
        <OPTION VALUE="Y">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.0.OPTION.0.#text}</OPTION>
        <OPTION VALUE="N">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.0.OPTION.1.#text}</OPTION>
        <FIELD_NAME>GPI_ENABLED</FIELD_NAME>
      </LEGENDS>
      <LEGENDS>
        <LBL></LBL>
        <OPTION VALUE="R">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.1.OPTION.0.#text}</OPTION>
        <OPTION VALUE="A">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.1.OPTION.1.#text}</OPTION>
        <OPTION VALUE="X">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.1.OPTION.2.#text}</OPTION>
        <FIELD_NAME>RECALL_STATUS</FIELD_NAME>
      </LEGENDS>

    </SUMBLOCK>
    <TYPE>S</TYPE>
  </SUMMARY>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>

      </TAB>
    </BODY>
  </SCREEN>
</FORM>