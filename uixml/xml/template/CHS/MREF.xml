                <SIZE></SIZE>
                <OPTION VALUE="ABI" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.0.#text}</OPTION>
                <OPTION VALUE="CI" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.1.#text}</OPTION>
                <OPTION VALUE="CIF" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.2.#text}</OPTION>
                <OPTION VALUE="ERI" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.3.#text}</OPTION>
                <OPTION VALUE="OTH" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.4.#text}</OPTION>
                <OPTION VALUE="STAT" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.5.#text}</OPTION>
                <OPTION VALUE="UATI" SELECTED="0">${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.OPTION.6.#text}</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>TABLENAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.2.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnRefreshMV(event);</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_REF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
