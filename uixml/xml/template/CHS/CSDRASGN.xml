<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_UI_FIELDS">
              <FIELD INDEX="" CONTROL="N">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.0.FIELD.0.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>WORK_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_USER_ID</NAME>
                </LOV>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.0.FIELD.1.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>USER_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.0.LBL}</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_UI_FIELDS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <FLDSET VIEW="SE" TYPE="SE" INDEX="2" ID="FST_CONTROL">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.1.FIELD.LBL}</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnReassign()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_REASSIGN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PROCESS_CODE</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>