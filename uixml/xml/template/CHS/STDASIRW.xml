      </FIELD>
      <FIELD TYPE="ME" ID="6">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.5.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
        <NAME>SWITCH_STATUS</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="7">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.6.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
        <NAME>LATEST_SWITCH_ACTION</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="8">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.7.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>7</MAXLENGTH>
        <NAME>NEXT_ACTION_DUE</NAME>
      </FIELD>

    </SUMBLOCK>
    <TYPE>S</TYPE>
  </SUMMARY>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="L">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>

      </TAB>
    </BODY>
  </SCREEN>
</FORM>