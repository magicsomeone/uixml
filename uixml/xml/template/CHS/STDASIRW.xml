<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SUMMARY CRITERIA_SRCH="N" POSITION="TEMPLATE" TITLE="${FORM.SUMMARY.-TITLE}" TMP_SCR_TYPE="M">
    <SUMMARY_BASE></SUMMARY_BASE>
    <SUMBUTTONS>
      <BUTTON_ROWS></BUTTON_ROWS>
      <BUTTON ID="1">
        <BUTTON_EVENT>fnDetail()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.0.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_DETAIL</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="2">
        <BUTTON_EVENT>fnAccept()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.1.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_ACCEPT</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="3">
        <BUTTON_EVENT>fnReject()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.2.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_REJECT</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="4">
        <BUTTON_EVENT>fnAuth()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.3.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_AUTH</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="5">
        <BUTTON_EVENT>fnBalTfrAck()</BUTTON_EVENT>
        <BUTTON_LBL>${FORM.SUMMARY.SUMBUTTONS.BUTTON.4.BUTTON_LBL}</BUTTON_LBL>
        <BUTTON_NAME>BTN_BTRS</BUTTON_NAME>
      </BUTTON>
      <BUTTONS_PER_ROW></BUTTONS_PER_ROW>
    </SUMBUTTONS>
    <SUMMARY_DATA_BLK>BLK_SUMMARY</SUMMARY_DATA_BLK>
    <SUMBLOCK TABPAGE="QUERY" TYPE="SE" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.0.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>18</MAXLENGTH>
        <NAME>UNQ_REF_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.1.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
        <NAME>SWITCH_TYPE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.2.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>20</MAXLENGTH>
        <NAME>OLD_AC_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.3.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>7</MAXLENGTH>
        <NAME>SWITCH_DATE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="5">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.4.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>105</MAXLENGTH>
        <NAME>OLD_AC_NAME</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="6">
        <LBL>${FORM.SUMMARY.SUMBLOCK.0.FIELD.5.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
        <NAME>SWITCH_STATUS</NAME>
      </FIELD>

    </SUMBLOCK>
    <SUMBLOCK TABPAGE="RESULT" TYPE="ME" SCREEN="SUMMARY">
      <FIELD TYPE="ME" ID="1">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.0.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>18</MAXLENGTH>
        <NAME>UNQ_REF_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.1.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
        <NAME>SWITCH_TYPE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.2.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>20</MAXLENGTH>
        <NAME>OLD_AC_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.LBL}</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>7</MAXLENGTH>
        <NAME>SWITCH_DATE</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="5">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.4.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>105</MAXLENGTH>
