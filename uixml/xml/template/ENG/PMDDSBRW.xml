        <OPTION VALUE="S" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.5.OPTION.1.#text}</OPTION>
        <OPTION VALUE="P" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.5.OPTION.2.#text}</OPTION>
