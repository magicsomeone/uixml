        <OPTION VALUE="P">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.1.OPTION.1.#text}</OPTION>
        <OPTION VALUE="W">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.1.OPTION.2.#text}</OPTION>
