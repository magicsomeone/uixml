<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_CUST_TEXT" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="S">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL>${FORM.SCREEN.BODY.TAB.LBL}</LBL>
        <SECTION ID="SEC1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="ME" INDEX="1" ID="FST_CUSTNO">
              <FIELD INDEX="" CONTROL="N">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.0.PART.FLDSET.FIELD.LBL}</LBL>
                <SIZE>9</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>9</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CUSTNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CUSTTEXT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC2" COLLAPSE="N">
          <PART NAME="PART2" ID="PART2" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="ME" INDEX="1" ID="FST_SEQ">
              <NAV_BTN_REQ>Y</NAV_BTN_REQ>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>3</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>SEQNO</NAME>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.1.PART.FLDSET.FIELD.0.LBL}</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC></OK_IMG_SRC>
                  <TITLE>${FORM.SCREEN.BODY.TAB.SECTION.1.PART.FLDSET.FIELD.1.POPUPEDIT.TITLE}</TITLE>
                  <CANCEL_IMG_SRC></CANCEL_IMG_SRC>
                </POPUPEDIT>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.1.PART.FLDSET.FIELD.1.LBL}</LBL>
                <SIZE>800</SIZE>
                <DTYPE>CLOB</DTYPE>
                <COLS>80</COLS>
                <TYPE>TEXTAREA</TYPE>
                <MAXLENGTH>4000</MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnValidateLength()</FUNCTION>
                  <NAME>onchange</NAME>
                </EVENT>
                <NAME>TDESC</NAME>
                <ROWS>10</ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CUSTTEXT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>