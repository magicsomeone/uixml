        <OPTION VALUE="Y" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.1.OPTION.1.#text}</OPTION>
        <OPTION VALUE="N" SELECTED="0">${FORM.SUMMARY.SUMBLOCK.1.FIELD.1.OPTION.2.#text}</OPTION>
        <DTYPE>CHAR</DTYPE>
        <TYPE>SELECT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>IS_FORGOTTEN</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.2.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>1</MAXLENGTH>
        <NAME>PROCESS_STATUS</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="4">
        <LBL>${FORM.SUMMARY.SUMBLOCK.1.FIELD.3.LBL}</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4000</MAXLENGTH>
        <NAME>ERROR_DESC</NAME>
      </FIELD>
      <LEGENDS>
        <LBL></LBL>
        <OPTION VALUE="Y">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.OPTION.0.#text}</OPTION>
        <OPTION VALUE="N">${FORM.SUMMARY.SUMBLOCK.1.LEGENDS.OPTION.1.#text}</OPTION>
        <FIELD_NAME>IS_FORGOTTEN</FIELD_NAME>
      </LEGENDS>

    </SUMBLOCK>
