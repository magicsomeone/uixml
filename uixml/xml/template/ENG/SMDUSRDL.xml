<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="${FORM.SCREEN.-TITLE}" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL/>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL/>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL/>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_USER">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_USER</NAME>
                </LOV>
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.0.LBL}</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>USER_ID</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.1.LBL}</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>USER_NAME</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>${FORM.SCREEN.BODY.TAB.SECTION.PART.FLDSET.FIELD.2.LBL}</LBL>
                <SIZE/>
                <DTYPE/>
                <COLS/>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH/>
                <EVENT>
                  <FUNCTION>fnDelete()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_DELETE</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_USER</BLOCK>
              <WIDTH/>
              <HEIGHT/>
            </FLDSET>

          </PART>
          <LBL/>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>