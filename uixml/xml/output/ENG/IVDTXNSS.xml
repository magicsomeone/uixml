    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL/>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL/>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL/>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="P1" ID="P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MASTER">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_BRANCH</NAME>
                </LOV>
                <LBL>Branch Code</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>BRANCH_CODE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Branch Description</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>BRANCH_DESC</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>From Date</LBL>
                <SIZE/>
                <DTYPE>DATE</DTYPE>
                <COLS/>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>FROM_DATE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD CONTROL="N" SPRT="1" INDEX="">
                <SIZE/>
                <VALUE>ALL</VALUE>
                <COLS/>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>INSTRUMENT_TYPE</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_INSTR_TYPE</NAME>
                </LOV>
                <LBL>Instrument type</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Quantity</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>QUANTITY</NAME>
                <ROWS/>
              </FIELD>
              <FIELD CONTROL="N" SPRT="2" INDEX="">
                <SIZE/>
                <VALUE>global.UserId</VALUE>
                <COLS/>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>TELLER_ID</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_TELLER_ID</NAME>
                </LOV>
                <LBL>Teller Id</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Status</LBL>
                <SIZE/>
                <OPTION VALUE="A" SELECTED="0">Unused</OPTION>
                <OPTION VALUE="U" SELECTED="0">Used</OPTION>
                <OPTION VALUE="I" SELECTED="0">Issued</OPTION>
                <OPTION VALUE="B" SELECTED="0">Blocked</OPTION>
                <OPTION VALUE="D" SELECTED="0">Destroyed</OPTION>
                <OPTION VALUE="T" SELECTED="0">Transfered</OPTION>
                <OPTION VALUE="R" SELECTED="0">Returned</OPTION>
                <OPTION VALUE="O" SELECTED="0">Issued to User Unauthorised</OPTION>
                <OPTION VALUE="P" SELECTED="0">Destroyed Unauthorised</OPTION>
                <OPTION VALUE="Q" SELECTED="0">Blocked Unauthorised</OPTION>
                <OPTION VALUE="S" SELECTED="0">Unblocked Unauthorised</OPTION>
                <OPTION VALUE="Y" SELECTED="0">Issued to Customer Unauthorised</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>To Date</LBL>
                <SIZE/>
                <DTYPE>DATE</DTYPE>
                <COLS/>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>TO_DATE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
                <LBL>Query</LBL>
                <SIZE/>
                <DTYPE/>
                <COLS/>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH/>
                <EVENT>
                  <FUNCTION>fnQuery();</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_Q</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_SUMMARY_MASTER</BLOCK>
              <WIDTH/>
              <HEIGHT/>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL/>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
