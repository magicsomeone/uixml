                <LBL>Quantity</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>QUANTITY</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>From Serial Number</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>FROM_SR_NO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>To  Serial Number</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>TO_SRNO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Instrument type</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>INSTRUMENT_TYPE</NAME>
                <ROWS/>
              </FIELD>
              <LBL>Transaction detail</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_SUMMARY_DETAILS</BLOCK>
              <WIDTH/>
              <PGSIZE>15</PGSIZE>
              <HEIGHT/>
            </FLDSET>

          </PART>
          <LBL/>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>