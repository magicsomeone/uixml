<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Journal Batch Browser" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL>Journal Batch Browser</LBL>
        <SECTION ID="SEC_BATBROW" COLLAPSE="N">
          <PART NAME="BATCH" ID="BATCH" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_BATCH">
              <FIELD CONTROL="N" SPRT="1" INDEX="">
                <SIZE>3</SIZE>
                <HIDDEN>-1</HIDDEN>
                <COLS></COLS>
                <RELATED_FIELD>BRN</RELATED_FIELD>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>BRN</NAME>
                <LBL>Branch</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>HIDDEN</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" SPRT="1" INDEX="">
                <SIZE>4</SIZE>
                <COLS></COLS>
                <RELATED_FIELD>BATNO</RELATED_FIELD>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>BATNO</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_BATCH</NAME>
                </LOV>
                <LBL>Batch Number</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" SPRT="2" INDEX="">
                <SIZE>12</SIZE>
                <COLS></COLS>
                <MAXLENGTH>12</MAXLENGTH>
                <NAME>LASOPID</NAME>
                <CASE>UPPER</CASE>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_OPERATOR</NAME>
                </LOV>
                <LBL>Last Operator Id</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Authorization Status</LBL>
                <SIZE>1</SIZE>
                <OPTION VALUE="U" SELECTED="0">Unauthorized</OPTION>
                <OPTION VALUE="A" SELECTED="0">Authorized</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>AUTH_STAT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Check Total</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="Y" SELECTED="0">Balanced</OPTION>
                <OPTION VALUE="N" SELECTED="0">Un-Balanced</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>ENTRIES_TOT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_BATCH_BROWSER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_BATBROWDTLS" COLLAPSE="N">
          <PART NAME="BATBROWDTLS" ID="BATBROWDTLS" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_BATBROWDTLS">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Batch Number</LBL>
                <SIZE>4</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>BATNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Batch Type</LBL>
                <SIZE>1</SIZE>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Operator</LBL>
                <SIZE>12</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <NAME>LASOPID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Authorizer</LBL>
                <SIZE>12</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <NAME>LASAUID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>22</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>DEBT</NAME>
                <LBL>Local Currency Debit</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>22</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>CRDT</NAME>
                <LBL>Local Currency Credit</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>22</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>DIFF</NAME>
                <LBL>Difference</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Batch Browser</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_BATCH_BROWSER_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_AUTHSTAT" COLLAPSE="N">
          <PART NAME="AUTHSTAT" ID="AUTHSTAT" WIDTH="100">
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>