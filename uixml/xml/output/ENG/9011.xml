<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Buy TC From Agent" TMP_SCR_TYPE="S">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL>All</LBL>
        <SECTION ID="SEC_SEC1" COLLAPSE="N">
          <PART NAME="SEC1_PART1" ID="SEC1_PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N">
                <LBL>External Reference Number</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>XREF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <OFFLINE_LOV>
                  <TYPE>G</TYPE>
                  <NAME>LOV_CURRENCY_LOCAL</NAME>
                </OFFLINE_LOV>
                <SIZE>3</SIZE>
                <COLS></COLS>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>TXNCCY</NAME>
                <CASE>UPPER</CASE>
                <LBL>TC Currency</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <SIZE>3</SIZE>
                <COLS></COLS>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CLGBNK</NAME>
                <CASE>UPPER</CASE>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CLGBNK</NAME>
                </LOV>
                <LBL>Issuer Code</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="SEC1_PART2" ID="SEC1_PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_2">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Branch Code</LBL>
                <SIZE>3</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>BRN</NAME>
                <CASE>UPPER</CASE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <RELATED_FIELD>TXNCCY</RELATED_FIELD>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>TXNAMT</NAME>
                <LBL>TC Amount</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>AMOUNT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC></OK_IMG_SRC>
                  <TITLE>Narrative</TITLE>
                  <CANCEL_IMG_SRC></CANCEL_IMG_SRC>
                </POPUPEDIT>
                <LBL>Narrative</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>255</MAXLENGTH>
                <NAME>NARRATIVE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_TCDENOM">
        <LBL></LBL>

      </TAB>
    </BODY>
    <CALLFORMS>
      <FORM SEQ="1" id="TCCN">
        <DISP_TAB>TAB_TCDENOM</DISP_TAB>
        <DISP_TYPE>TAB</DISP_TYPE>
        <FUNCTION>TCCN</FUNCTION>
      </FORM>
    </CALLFORMS>
  </SCREEN>
</FORM>