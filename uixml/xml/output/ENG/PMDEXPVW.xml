<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_EXCEPTION_LOG">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Exception Reference Number</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>50</MAXLENGTH>
                <NAME>EXCEPTION_REF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Queue Name</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>50</MAXLENGTH>
                <NAME>Q_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Date Logged</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATETIME</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>LOG_DATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Queue Code</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>2</MAXLENGTH>
                <NAME>QUEUE_CODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Status</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="P" SELECTED="0">Processed</OPTION>
                <OPTION VALUE="U" SELECTED="0">Unprocessed</OPTION>
                <OPTION VALUE="I" SELECTED="0">Ignore</OPTION>
                <OPTION VALUE="R" SELECTED="0">Repair</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PMTM_EXCEPTION_LOG</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MSG">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Response Message</LBL>
                <SIZE></SIZE>
                <DTYPE>CLOB</DTYPE>
                <COLS>90</COLS>
                <TYPE>TEXTAREA</TYPE>
                <MAXLENGTH>4000</MAXLENGTH>
                <NAME>MSG</NAME>
                <ROWS>30</ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PMTM_EXCEPTION_LOG</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>