<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Payment Authorization" TMP_SCR_TYPE="S">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="SEC1_PART1" ID="SEC1_PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MAIN">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>Down Payment Reference</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PREFNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Financing Account Reference</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DPREFNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Branch</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>BRN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CIVWS_DP_AUTH</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="SEC1_PART2" ID="SEC1_PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MAIN2">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Financing Account Currency</LBL>
                <SIZE>3</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CCY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <RELATED_FIELD>CCY</RELATED_FIELD>
                <MAXLENGTH></MAXLENGTH>
                <NAME>DPAMT</NAME>
                <LBL>Amount</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>AMOUNT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Downpayment Value Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>VALDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Customer ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>9</MAXLENGTH>
                <NAME>CUST</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CIVWS_DP_AUTH</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="SEC2_PART1" ID="SEC2_PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_OVERRIDE">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Confirmed</LBL>
                <SIZE></SIZE>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>CHECKBOX</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>CONFIRMED</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC></OK_IMG_SRC>
                  <TITLE>Error Message</TITLE>
                  <CANCEL_IMG_SRC></CANCEL_IMG_SRC>
                </POPUPEDIT>
                <LBL>Error Message</LBL>
                <SIZE>50</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>OVDMSGS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC></OK_IMG_SRC>
                  <TITLE>Remarks</TITLE>
                  <CANCEL_IMG_SRC></CANCEL_IMG_SRC>
                </POPUPEDIT>
                <LBL>Remarks</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>255</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>OVDREMARKS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Status</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>TXTSTATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Checker</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>AUTHBY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>AUTHDTSTAMP</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CSVWS_OVDS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_3" COLLAPSE="N">
          <PART NAME="SEC3_PART1" ID="SEC3_PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_AUTH">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Authorize</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnOnlineAuth()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_AUTH</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CIVWS_DP_AUTH</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>