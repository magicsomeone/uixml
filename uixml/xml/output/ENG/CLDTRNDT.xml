<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN QUERY_REQD="N" MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="" TMP_SCR_TYPE="L">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <SUBSCREEN>
      <FORM TYPE="C" SEQ="1" id="">
        <SCREEN>CVS_MAINAC</SCREEN>
        <LBL>Loan Master</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="C" SEQ="2" id="">
        <SCREEN>CVS_MAINDSB</SCREEN>
        <LBL>Loan Disbursement</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="C" SEQ="3" id="">
        <SCREEN>CVS_MAINPAY</SCREEN>
        <LBL>Loan Payment</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="C" SEQ="4" id="">
        <SCREEN>CVS_MAINVAMI</SCREEN>
        <LBL>Loan Amendments</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="C" SEQ="5" id="">
        <SCREEN>CVS_MAINHIST</SCREEN>
        <LBL>Account History</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="LF" SEQ="6" id="CLDEVTQR">
        <SCREEN>CVS_EVENT</SCREEN>
        <LBL>Events Diary</LBL>
        <FUNCTION>CLDEVTQR</FUNCTION>
      </FORM>
      <FORM TYPE="LF" SEQ="7" id="CLDINSQR">
        <SCREEN>CVS_INVQRY</SCREEN>
        <LBL>Installment Query</LBL>
        <FUNCTION>CLDINSQR</FUNCTION>
      </FORM>
      <FORM TYPE="LF" SEQ="8" id="CLDLSTMT">
        <SCREEN>CVS_LN_STMT</SCREEN>
        <LBL>Loan Statement</LBL>
        <FUNCTION>CLDLSTMT</FUNCTION>
      </FORM>
    </SUBSCREEN>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="SEC1_PAR1" ID="SEC1_PAR1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_TRN_MAIN">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Account Number</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>ACCOUNT_NUMBER</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Branch Code</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>BRANCH_CODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Module Code</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>2</MAXLENGTH>
                <NAME>MODULE_CODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRN_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="Y">
          <PART NAME="SEC2_PAR1" ID="SEC2_PAR1" WIDTH="66">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_TRN_DET">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Value Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>TRNDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Event</LBL>
                <SIZE>35</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>EVENT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Currency</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CCY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <RELATED_FIELD>CCY</RELATED_FIELD>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>CRAMT</NAME>
                <LBL>Credit Amount</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>AMOUNT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <RELATED_FIELD>CCY</RELATED_FIELD>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>DRAMT</NAME>
                <LBL>Debit Amount</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>AMOUNT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Loan Transaction Details</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRN_DET</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT>400</HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="SEC2_PAR2" ID="SEC2_PAR2" WIDTH="33">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_TRN_DATE">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Period</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="D" SELECTED="0">Date Range</OPTION>
                <OPTION VALUE="C" SELECTED="0">Current Month</OPTION>
                <OPTION VALUE="P" SELECTED="0">Current + Previous Month</OPTION>
                <OPTION VALUE="T" SELECTED="0">Current + Previous 2 Months</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <EVENT>
                  <FUNCTION>fn_change()</FUNCTION>
                  <NAME>onchange</NAME>
                </EVENT>
                <NAME>PERIOD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>From Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>FROMDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>To Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>TODT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>>></LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fn_period_change()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_VIEW</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Account Statement</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRN_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL>Loan Transaction Details</LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>