<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_VENDOR" POSITION="template" TITLE="Vendor Restriction Details" TMP_SCR_TYPE="M">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="All">
        <LBL>All</LBL>
        <SECTION ID="VEND_SEC1" COLLAPSE="N">
          <PART NAME="VEND_SEC1_PART1" ID="VEND_SEC1_PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_PRDCD">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product Code</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product Description</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_VENDOR</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="VEND_SEC2" COLLAPSE="N">
          <PART NAME="VEND_SEC2_PART1" ID="VEND_SEC2_PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_VENDORDETAILS">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product Code</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <SIZE>12</SIZE>
                <COLS></COLS>
                <MAXLENGTH>12</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>VENID</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_VENDOR</NAME>
                </LOV>
                <LBL>Vendor Id</LBL>
                <INPUT_LOV>-1</INPUT_LOV>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Vendor Name</LBL>
                <SIZE>30</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>VENDNAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Status Movement</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <VALUE>RESTRICTED</VALUE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Vendor Restriction</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_VENDOR_RESTRICT_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>