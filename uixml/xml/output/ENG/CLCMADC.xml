                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>30</MAXLENGTH>
                <NAME>RULE_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <SIZE></SIZE>
                <VALUE>N</VALUE>
                <COLS></COLS>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>DEFAULT_RULE</NAME>
                <LBL>Default Rule</LBL>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>CHECKBOX</TYPE>
                <ROWS></ROWS>
