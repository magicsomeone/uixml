        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CONDITION">
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CONDITION</NAME>
                </LOV>
                <LBL>Fields</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>FIELD_LIST</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="2">
                <LBL>Operator</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="=" SELECTED="0">Equal To</OPTION>
                <OPTION VALUE="&gt;" SELECTED="0">Greater Than</OPTION>
                <OPTION VALUE="&gt;=" SELECTED="0">Greater Than Or Equal To</OPTION>
                <OPTION VALUE="&lt;" SELECTED="0">Less Than</OPTION>
                <OPTION VALUE="&lt;=" SELECTED="0">Less Than Or Equal To</OPTION>
                <OPTION VALUE="Like" SELECTED="0">Like</OPTION>
                <OPTION VALUE="Not Like" SELECTED="0">Not Like</OPTION>
                <OPTION VALUE="&lt;&gt;" SELECTED="0">Not Equal To</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>OPERATOR</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="2">
                <LBL>Value</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>VALUE_TEXT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
