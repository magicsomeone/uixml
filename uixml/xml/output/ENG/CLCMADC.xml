                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXTAREA</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>CONDITION</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="2">
                <LBL>And</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>btnAnd()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_AND</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
                <LBL>Element</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="DAYS" SELECTED="0">Days</OPTION>
                <OPTION VALUE="YEAR" SELECTED="0">Year</OPTION>
                <OPTION VALUE="CUSTOMER_CATEGORY" SELECTED="0">Customer Category</OPTION>
                <OPTION VALUE="PRINCIPAL" SELECTED="0">Principal</OPTION>
                <OPTION VALUE="TENOR" SELECTED="0">Tenor</OPTION>
                <OPTION VALUE="LOAN_STATUS" SELECTED="0">Loan Status</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>LST_ELEM</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="2">
                <LBL>Or</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>btnOr()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_OR</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
                <LBL>Logical Op</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="&lt;" SELECTED="0"><</OPTION>
                <OPTION VALUE="&gt;" SELECTED="0">></OPTION>
                <OPTION VALUE="=" SELECTED="0">=</OPTION>
                <OPTION VALUE="&lt;&gt;" SELECTED="0"><></OPTION>
                <OPTION VALUE="&lt;=" SELECTED="0"><=</OPTION>
                <OPTION VALUE="&gt;=" SELECTED="0">>=</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>LST_LOP</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="2">
                <LBL>Accept</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>btnAccept()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_ACCEPT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
