<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MADCSC" POSITION="template" TITLE="Minimum Amount Due Calculation Method" TMP_SCR_TYPE="S">
