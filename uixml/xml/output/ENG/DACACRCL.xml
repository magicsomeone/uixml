    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="All">
        <LBL>All</LBL>
        <SECTION ID="SEC_ALL_1" COLLAPSE="N">
          <PART NAME="PART_ALL_1" ID="PART_ALL_1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_PRODUCT_CODE">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Product Code</LBL>
                <SIZE>4</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Product Description</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PRODUCT_DISCOUNT_ACCRUAL</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_ALL_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CLASS_CODE">
              <FIELD CONTROL="N" INDEX="">
                <SIZE></SIZE>
                <COLS></COLS>
                <MAXLENGTH>10</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CLASCD</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CLASS_CODE</NAME>
                </LOV>
                <LBL>Class Code</LBL>
                <INPUT_LOV>-1</INPUT_LOV>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Class Type</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>2</MAXLENGTH>
                <NAME>CLASTYP</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>PRD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Class Description</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CLASDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PRODUCT_DISCOUNT_ACCRUAL</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CLASS_BUTTON">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Default From Class</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnClassDefault('BLK_PRODUCT_DISCOUNT_ACCRUAL')</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_CLASDFLT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PRODUCT_DISCOUNT_ACCRUAL</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_ALL_3" COLLAPSE="N">
          <PART NAME="PART_ALL_ID_3.1" ID="PART_ALL_ID_3.1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_ACC_FREQ">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Discount Accrual Required</LBL>
                <SIZE></SIZE>
                <CUSTOM/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>CHECKBOX</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>DISCACCRREQD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Frequency</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="D" SELECTED="0">Daily</OPTION>
                <OPTION VALUE="M" SELECTED="0">Monthly</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>ACCRFREQ</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Handling of Fore Closure</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="C" SELECTED="0">Complete Accrual</OPTION>
                <OPTION VALUE="R" SELECTED="0">Refund</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PREPAYTBDTREAT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Acquisition Type</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="R" SELECTED="0">Par</OPTION>
                <OPTION VALUE="D" SELECTED="0">Par/Discount</OPTION>
                <OPTION VALUE="P" SELECTED="0">Par/Premium</OPTION>
                <OPTION VALUE="A" SELECTED="0">Par/Discount/Premium</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>ACQTYP</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Accrual Preference</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DISC_ACCR_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_ALL_4" COLLAPSE="N">
          <PART NAME="PART_ALL_ID_4.1" ID="PART_ALL_ID_4.1" WIDTH="66">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_DISCACCRCCYPREFERENCES">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>Product Code</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CCY</NAME>
                </LOV>
                <LBL>Currency Code</LBL>
                <SIZE>5</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CCYCODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Currency Name</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CCYNAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Numerator Method/Denominator Method</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="A" SELECTED="0">Actual</OPTION>
                <OPTION VALUE="E" SELECTED="0">30 Euro</OPTION>
                <OPTION VALUE="U" SELECTED="0">30-US</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>NUMDCNTMET</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Denominator Method</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="L" SELECTED="0">360</OPTION>
                <OPTION VALUE="H" SELECTED="0">365</OPTION>
                <OPTION VALUE="X" SELECTED="0">364</OPTION>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>DENOMDCNTMET</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Day Count Method</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CCY_PREFERENCES</BLOCK>
              <WIDTH>515</WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT>200</HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART_ALL_ID_4.2" ID="PART_ALL_ID_4.2" WIDTH="33"/>
          <LBL></LBL>

        </SECTION>
