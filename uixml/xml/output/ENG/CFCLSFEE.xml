<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_CFCLSFEE" POSITION="template" TITLE="Ad hoc Charges" TMP_SCR_TYPE="S">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="All">
        <LBL>All</LBL>
        <SECTION ID="S1_HEAD" COLLAPSE="N">
          <PART NAME="S1_HEAD_P1" ID="S1_HEAD_P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FLDSET_ONE">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product Code</LBL>
                <SIZE>4</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Product Description</LBL>
                <SIZE>50</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PRDDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Report Module</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>2</MAXLENGTH>
                <NAME>MODU</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_PRODUCT_ADHOC</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="S2_HEAD" COLLAPSE="N">
          <PART NAME="S2_HEAD_P1" ID="S2_HEAD_P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="ME" INDEX="1" ID="FLDSET_TWO">
              <NAV_BTN_REQ>Y</NAV_BTN_REQ>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <SIZE>10</SIZE>
                <COLS></COLS>
                <MAXLENGTH>10</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>COMPONENT</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_COMPONENT</NAME>
                </LOV>
                <LBL>Component</LBL>
                <INPUT_LOV>-1</INPUT_LOV>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Class Description</LBL>
                <SIZE>50</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CLASDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <SIZE>1</SIZE>
                <COLS></COLS>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnChangeCheck()</FUNCTION>
                  <NAME>onchange</NAME>
                </EVENT>
                <NAME>PROP_REQD</NAME>
                <LBL>Propagation Required</LBL>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>CHECKBOX</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_ADHOCFEE</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>