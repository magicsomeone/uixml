                <OPTION VALUE="mod11" SELECTED="0">Modulo 11</OPTION>
                <OPTION VALUE="mod97" SELECTED="0">Modulo 97</OPTION>
                <OPTION VALUE="mod11WW" SELECTED="0">Modulo 11 with Weights</OPTION>
