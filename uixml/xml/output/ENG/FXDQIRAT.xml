<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Periodic Rate Update" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_EXCHRATE">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_RATETYPE</NAME>
                </LOV>
                <LBL>Rate Type</LBL>
                <SIZE>10</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>RATETYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Exchange Rate</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DISCOUNTRATES</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CCY">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CCY</NAME>
                </LOV>
                <LBL>Currency</LBL>
                <SIZE>5</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>CURRENCY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DISCOUNTRATES</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_TENOR">
              <FIELD INDEX="" CONTROL="N">
                <LBL></LBL>
                <SIZE></SIZE>
                <OPTION COL="1" ROW="LBL_DATES">
                  <SELECTED>-1</SELECTED>
                  <LBL>Date</LBL>
                  <ID>D</ID>
                  <VALUE>D</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_TENOR">
                  <SELECTED>0</SELECTED>
                  <LBL>Tenor</LBL>
                  <ID>T</ID>
                  <VALUE>T</VALUE>
                </OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <VALUE>D</VALUE>
                <COLS></COLS>
                <TYPE>RADIO</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>TENORDAYS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>Start Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>STARTDAT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>End Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>ENDDAT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Tenor</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DISCOUNTRATES</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_QUERY">
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>5</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>DAYS</NAME>
                <LBL>Days</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Query</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fn_qry()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_QUERY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Discount Rate</LBL>
                <SIZE>10</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DISCOUNTRATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DISCOUNTRATES</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>