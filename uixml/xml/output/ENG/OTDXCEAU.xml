<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_AUTHMAIN" POSITION="template" TITLE="Contract Exercise Authorization" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>
        <SECTION ID="SEC_ONE" COLLAPSE="N">
          <PART NAME="PART_ONE" ID="PART_ONE" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>Contract Reference</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>64</MAXLENGTH>
                <NAME>FCCREF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Event Code</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <NAME>EVENTCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_OEAD</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART_TWO" ID="PART_TWO" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_2">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Event Date</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>EVENTDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Input By</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>48</MAXLENGTH>
                <NAME>INPUTBY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_OEAD</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL>Rekey Details</LBL>
        <SECTION ID="SEC_TWO" COLLAPSE="N">
          <PART NAME="PART_ONE" ID="PART_ONE" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_3">
              <FIELD INDEX="" CONTROL="N">
                <LBL>LBL_FCCREFE</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>64</MAXLENGTH>
                <NAME>FCCREFE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>SWVAL</NAME>
                <LBL>Swap Value</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>REFRT</NAME>
                <LBL>Reference Rate</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Rekey Details</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_OERD</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART_TWO" ID="PART_TWO" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_5">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Authorize</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnOnlineAuth()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_AUTH</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_OEAD</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_THREE" COLLAPSE="N">
          <PART NAME="PART_ONE" ID="PART_ONE" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_4">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Confirmed</LBL>
                <SIZE></SIZE>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>CHECKBOX</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>CONFIRMED</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Overrides</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>2000</MAXLENGTH>
                <NAME>OCDTXT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Authorized By</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>48</MAXLENGTH>
                <NAME>AUTHBY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Remarks</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1020</MAXLENGTH>
                <NAME>REMARKS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Contract Overrides</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CVCONTOVD</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>