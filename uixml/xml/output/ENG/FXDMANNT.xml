<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Manual Netting" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CUSTOMER">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CUST_NO</NAME>
                </LOV>
                <LBL>Customer</LBL>
                <SIZE>15</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>CUSTOMER</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CURRENCY</NAME>
                </LOV>
                <LBL>Currency</LBL>
                <SIZE>5</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>CURRENCY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MANUAL_NETTING</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_NETTING">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>Netting Deal Type</LBL>
                <SIZE>1</SIZE>
                <OPTION VALUE="A" SELECTED="0">All Deals</OPTION>
                <OPTION VALUE="S" SELECTED="0">Spot</OPTION>
                <OPTION VALUE="F" SELECTED="0">Forward</OPTION>
                <OPTION VALUE="D" SELECTED="0">NDF Deals</OPTION>
                <OPTION VALUE="N" SELECTED="0">Non NDF Deals</OPTION>
                <OPTION VALUE="P" SELECTED="0">Split Netting</OPTION>
                <OPTION VALUE="E" SELECTED="0">Net NDF/Non NDF</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>NETTINGDEALTYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MANUAL_NETTING</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CCYPAIR">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Currency 1</LBL>
                <SIZE>5</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>CURRENCY1</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CCY_PAIR</NAME>
                </LOV>
                <LBL>Currency 2</LBL>
                <SIZE>5</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>CURRENCY2</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Sequence Number</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>SEQUENCENUM</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>View Log</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnViewFXLog()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_FX_LOG</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Currency Pair</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MANUAL_NETTING</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>