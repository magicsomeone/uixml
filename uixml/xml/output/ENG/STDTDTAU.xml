<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_AUTH" POSITION="template" TITLE="Authorize" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_TOPUP" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_TOPUP">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Top-Up Reference Number</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>80</MAXLENGTH>
                <NAME>TOPUP_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>Branch</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <NAME>BRN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>Term Deposit Account Number</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>80</MAXLENGTH>
