<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="Dynamic Package/DML Execution Screen" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <SUBSCREEN>
      <FORM TYPE="S" SEQ="1" id="CVS_ERR">
        <SCREEN>CVS_ERR</SCREEN>
        <LBL>Error Details</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
    </SUBSCREEN>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PAR_1" ID="PAR_1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_DYN_MODULE</NAME>
                </LOV>
                <LBL>Module Code</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>MODULECODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Function Id</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>FUNCTIONID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Status</LBL>
                <SIZE></SIZE>
                <OPTION COL="1" ROW="LBL_UNPROCESSED">
                  <SELECTED>-1</SELECTED>
                  <LBL>Unprocessed</LBL>
                  <ID>U</ID>
                  <VALUE>U</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_PROCESSED">
                  <SELECTED>0</SELECTED>
                  <LBL>Processed</LBL>
                  <ID>P</ID>
                  <VALUE>P</VALUE>
                </OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <VALUE>U</VALUE>
                <COLS></COLS>
                <TYPE>RADIO</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Populate</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fn_fetch()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_POPULATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Dynamic Package/DML Execution Fetch</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MASTER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PAR_1" ID="PAR_1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_2">
              <FIELD INDEX="" CONTROL="N">
                <LBL>Module</LBL>
                <SIZE>10</SIZE>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>2</MAXLENGTH>
                <NAME>MODULE_CODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Function Id</LBL>
                <SIZE>10</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>FUNCTION_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Function Type</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>FUNCTION_TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Function Param</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>FUNCTION_PARAM</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Version</LBL>
                <SIZE>10</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>VERSION_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>Status</LBL>
                <SIZE></SIZE>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>Dynamic Package/DML Execution Records</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CHILD</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_3" COLLAPSE="N">
          <PART NAME="PAR_1" ID="PAR_1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_3">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>Process All Records</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fn_process_all()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_PROCESS_ALL</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MASTER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
