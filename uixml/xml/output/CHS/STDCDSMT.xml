<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_ADHOC_COMBSTMT" POSITION="template" TITLE="" TMP_SCR_TYPE="S">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <SUBSCREEN>
      <FORM TYPE="LF" SEQ="1" id="STRCDSMT">
        <SCREEN>CVS_MAIN</SCREEN>
        <LBL>报表</LBL>
        <FUNCTION>STRCDSMT</FUNCTION>
      </FORM>
      <FORM TYPE="C" SEQ="2" id="CSCONCHG">
        <SCREEN>CVS_CHARGE</SCREEN>
        <LBL>计费</LBL>
        <FUNCTION>CSCONCHG</FUNCTION>
      </FORM>
    </SUBSCREEN>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CUSTOMER">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CUSTNO</NAME>
                </LOV>
                <LBL>客户编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>CUST_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>计费账户</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <NAME>CHARGE_ACC</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>DCN</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>200</MAXLENGTH>
                <NAME>DCN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>计费参考编号</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <NAME>CHG_TRN_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>客户名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CUSTOMER_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_ADHOC_COMBSTMT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART_2" ID="PART_2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_STMTID">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_STMTID</NAME>
                </LOV>
                <LBL>报表 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>STMT_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>余额</LBL>
                <SIZE></SIZE>
                <OPTION COL="1" ROW="LBL_BOOK_DATED">
                  <SELECTED>-1</SELECTED>
                  <LBL>已标日期的账簿</LBL>
                  <ID>B</ID>
                  <VALUE>B</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_VALUE_DATED">
                  <SELECTED>0</SELECTED>
                  <LBL>起息日</LBL>
                  <ID>V</ID>
                  <VALUE>V</VALUE>
                </OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <VALUE>B</VALUE>
                <COLS></COLS>
                <TYPE>RADIO</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>BD_VD_BAL</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_ADHOC_COMBSTMT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_DATERANGE">
              <FIELD INDEX="" CONTROL="N">
                <LBL>起始日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>FROMDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>终止日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>TODT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>基于日期范围</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_ADHOC_COMBSTMT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART_2" ID="PART_2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_PRINTOPTION">
              <FIELD INDEX="" CONTROL="N">
                <LBL>打印选项</LBL>
                <SIZE>1</SIZE>
                <OPTION COL="1" ROW="LBL_VIEW">
                  <SELECTED>-1</SELECTED>
                  <LBL>查看</LBL>
                  <ID>V</ID>
                  <VALUE>V</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_PRINT">
                  <SELECTED>0</SELECTED>
                  <LBL>打印</LBL>
                  <ID>P</ID>
                  <VALUE>P</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_SERVER_SPOOL">
                  <SELECTED>0</SELECTED>
                  <LBL>服务器假脱机</LBL>
                  <ID>S</ID>
                  <VALUE>S</VALUE>
                </OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <VALUE>V</VALUE>
                <COLS></COLS>
                <TYPE>RADIO</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PRINTOT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <SIZE></SIZE>
                <VALUE>N</VALUE>
                <COLS></COLS>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>APPLY_ONLN_CHGS</NAME>
                <LBL>联机交易收费</LBL>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>CHECKBOX</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>发送至客户</LBL>
                <SIZE></SIZE>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>CHECKBOX</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>SEND_TO_CUST</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_ADHOC_COMBSTMT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>