                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_2">
              <FIELD CONTROL="N" INDEX="">
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <VALUE>BCA</VALUE>
                <COLS></COLS>
                <MAXLENGTH>16</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>INSTRTYPE</NAME>
                <LBL>金融工具类型</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>HIDDEN</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>发行分行</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>ISSBRN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_INSTRNO</NAME>
                </LOV>
                <LBL>金融工具编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <NAME>INSTRNO</NAME>
                <CASE>UPPER</CASE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>计费币种</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>9</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>TXNCCY</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50"></PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50"></PART>
