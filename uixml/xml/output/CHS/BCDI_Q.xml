<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="重复发行 BC 金融工具" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_ALL">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N">
                <LBL>外部参考编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>XREF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>付款模式</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="G" SELECTED="0">总账</OPTION>
                <OPTION VALUE="C" SELECTED="0">现金</OPTION>
                <OPTION VALUE="A" SELECTED="0">账户</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
