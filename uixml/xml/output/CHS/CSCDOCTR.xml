                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>上载</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>btn_upload(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_UPLOAD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>查看</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>btn_view(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_VIEW</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>请求参考编号</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>REQ_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>单据上载</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DOC_UPLOAD</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
