                <LBL>关键字 ID</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>30</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>KEY_ID</NAME>
