                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>5</MAXLENGTH>
                <NAME>EXPLINE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>结果</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>2000</MAXLENGTH>
                <NAME>RSLT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>条件</LBL>
                <SIZE>15</SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnpopcond("CLTMS_CREDIT_SCORE_RULE_EXPR")</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_COND</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>条件</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4000</MAXLENGTH>
                <NAME>COND</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>规则名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>30</MAXLENGTH>
                <NAME>RULNAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>信用评分规则表达式</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CREDIT_SCORE_RULE_EXPR</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_PROD" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
