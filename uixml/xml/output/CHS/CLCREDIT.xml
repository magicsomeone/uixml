                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>默认规则</LBL>
                <SIZE></SIZE>
                <CUSTOM>
                  <OFF>N</OFF>
                  <ON>Y</ON>
                </CUSTOM>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>CHECKBOX</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>DFLTRULE</NAME>
