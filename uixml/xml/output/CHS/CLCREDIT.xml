    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_CRD1" COLLAPSE="N">
          <PART NAME="SEC_CRD1_PART1" ID="SEC_CRD1_PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="ME" INDEX="1" ID="FST_CRDRULE">
              <NAV_BTN_REQ>Y</NAV_BTN_REQ>
              <FIELD INDEX="" CONTROL="N">
