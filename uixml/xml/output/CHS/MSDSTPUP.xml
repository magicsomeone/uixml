<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="上载" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_SOURCECODE</NAME>
                </LOV>
                <LBL>来源代码</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>SOURCECODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>来源名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>SOURCENAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_MSG_TYPE</NAME>
                </LOV>
                <LBL>消息类型</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>MESSAGETYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_2">
              <FIELD INDEX="" CONTROL="Y">
                <LBL></LBL>
                <SIZE></SIZE>
                <OPTION COL="1" ROW="LBL_STP">
                  <SELECTED>-1</SELECTED>
                  <LBL>STP</LBL>
                  <ID>S</ID>
                  <VALUE>S</VALUE>
                </OPTION>
                <OPTION COL="1" ROW="LBL_MANUAL">
                  <SELECTED>0</SELECTED>
                  <LBL>手动</LBL>
                  <ID>M</ID>
                  <VALUE>M</VALUE>
                </OPTION>
                <DTYPE>CHAR</DTYPE>
                <VALUE>S</VALUE>
                <COLS></COLS>
                <TYPE>RADIO</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <NAME>STP_MANUAL</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>公布上载状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="H" SELECTED="0">保持</OPTION>
                <OPTION VALUE="U" SELECTED="0">未授权</OPTION>
                <OPTION VALUE="A" SELECTED="0">已授权</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>POSTUPLSTATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_3" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_3">
              <FIELD INDEX="" CONTROL="N">
                <LBL>错误时</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="R" SELECTED="0">拒绝</OPTION>
                <OPTION VALUE="H" SELECTED="0">保持</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>ONERROR</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>授权</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="R" SELECTED="0">拒绝</OPTION>
                <OPTION VALUE="H" SELECTED="0">保持</OPTION>
                <OPTION VALUE="I" SELECTED="0">忽略</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>ONOVERRIDE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>无受益人时</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="Y" SELECTED="0">暂记总账</OPTION>
                <OPTION VALUE="N" SELECTED="0">维修</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>WHNOBEN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>异常</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_4" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_4">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>上载</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnupload()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_UPLOAD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MAIN</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>