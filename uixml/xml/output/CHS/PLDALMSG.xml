<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MESGS" POSITION="template" TITLE="查看消息" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_1">
              <FIELD INDEX="" CONTROL="N">
                <LBL>交易参考编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CHAR_FIELD1</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MESSAGE_HEADER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_4">
              <FIELD INDEX="" CONTROL="N">
                <LBL>交易参考编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>40</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>TXN_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>消息参考编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>22</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>MESSAGE_REF_NO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>报文类型</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>MESSAGE_TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>消息状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="U" SELECTED="0">未处理</OPTION>
                <OPTION VALUE="P" SELECTED="0">已处理</OPTION>
                <OPTION VALUE="R" SELECTED="0">修复</OPTION>
                <OPTION VALUE="S" SELECTED="0">已停止</OPTION>
                <OPTION VALUE="G" SELECTED="0">已生成</OPTION>
                <OPTION VALUE="N" SELECTED="0">未生成</OPTION>
                <OPTION VALUE="H" SELECTED="0">已移交</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>MESSAGE_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>起息日</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>VALUE_DATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>解除日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>RELEASE_DATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>确认状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="A" SELECTED="0">已接受</OPTION>
                <OPTION VALUE="N" SELECTED="0">已拒绝</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>ACK_NAK_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>需要重复检查</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="Y" SELECTED="0">是</OPTION>
                <OPTION VALUE="N" SELECTED="0">否</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DUP_CHECK_REQUIRED</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>重复的状态</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DUPLICATE_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>指示</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="I" SELECTED="0">入</OPTION>
                <OPTION VALUE="O" SELECTED="0">传出</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DIRECTION</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MESSAGE_DTLS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_3" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_3">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>消息</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnMessage(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_MESG</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MESSAGE_HEADER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_5">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>确认</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnAck(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_ACK</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MESSAGE_HEADER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>