<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL/>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL/>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL/>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="P1" ID="P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MASTER">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_BRANCH</NAME>
                </LOV>
                <LBL>分行代码</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>BRANCH_CODE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>分行说明</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>BRANCH_DESC</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>起始日期</LBL>
                <SIZE/>
                <DTYPE>DATE</DTYPE>
                <COLS/>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>FROM_DATE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD CONTROL="N" SPRT="1" INDEX="">
                <SIZE/>
                <VALUE>ALL</VALUE>
                <COLS/>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>INSTRUMENT_TYPE</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_INSTR_TYPE</NAME>
                </LOV>
                <LBL>金融工具类型</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>数量</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>QUANTITY</NAME>
                <ROWS/>
              </FIELD>
              <FIELD CONTROL="N" SPRT="2" INDEX="">
                <SIZE/>
                <VALUE>global.UserId</VALUE>
                <COLS/>
                <MAXLENGTH>1000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>TELLER_ID</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_TELLER_ID</NAME>
                </LOV>
                <LBL>经办ID</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>状态</LBL>
                <SIZE/>
                <OPTION VALUE="A" SELECTED="0">未使用</OPTION>
                <OPTION VALUE="U" SELECTED="0">已使用</OPTION>
                <OPTION VALUE="I" SELECTED="0">已分配</OPTION>
                <OPTION VALUE="B" SELECTED="0">冻结</OPTION>
                <OPTION VALUE="D" SELECTED="0">损毁</OPTION>
                <OPTION VALUE="T" SELECTED="0">移转</OPTION>
                <OPTION VALUE="R" SELECTED="0">缴回</OPTION>
                <OPTION VALUE="O" SELECTED="0">分配给经办未授权</OPTION>
                <OPTION VALUE="P" SELECTED="0">损毁未授权</OPTION>
                <OPTION VALUE="Q" SELECTED="0">冻结未授权</OPTION>
                <OPTION VALUE="S" SELECTED="0">解除冻结未授权</OPTION>
                <OPTION VALUE="Y" SELECTED="0">分配给客户未授权</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>截止日期</LBL>
                <SIZE/>
                <DTYPE>DATE</DTYPE>
                <COLS/>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>TO_DATE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y" SPRT="1">
                <LBL>查询</LBL>
                <SIZE/>
                <DTYPE/>
                <COLS/>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH/>
                <EVENT>
                  <FUNCTION>fnQuery();</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_Q</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_SUMMARY_MASTER</BLOCK>
              <WIDTH/>
              <HEIGHT/>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL/>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_DETAILS">
              <FIELD INDEX="" CONTROL="N">
                <LBL>数量</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>QUANTITY</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>开始序号</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>FROM_SR_NO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>结束序号</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>TO_SRNO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>金融工具类型</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>INSTRUMENT_TYPE</NAME>
                <ROWS/>
              </FIELD>
              <LBL>交易详细信息</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_SUMMARY_DETAILS</BLOCK>
              <WIDTH/>
              <PGSIZE>15</PGSIZE>
              <HEIGHT/>
            </FLDSET>

          </PART>
          <LBL/>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>