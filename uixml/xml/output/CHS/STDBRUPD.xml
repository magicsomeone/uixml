<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="分行转账表" TMP_SCR_TYPE="M">
    <EXIT_BTN>2</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="P1" ID="P1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_FIELD">
              <FIELD CONTROL="N" INDEX="">
                <SIZE></SIZE>
                <OPTION VALUE="C" SELECTED="0">账户</OPTION>
                <OPTION VALUE="L" SELECTED="-1">贷款</OPTION>
                <VALUE>L</VALUE>
                <COLS></COLS>
                <MAXLENGTH>20</MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnCallBackEnd()</FUNCTION>
                  <NAME>onchange</NAME>
                </EVENT>
                <NAME>LOANORCASA</NAME>
                <LBL>贷款或活期存款</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>SELECT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>-1</HREQ>
              <BLOCK>BLK_VIEW</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_2" COLLAPSE="N">
          <PART NAME="P2" ID="P2" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_LIST">
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_GET_TABLENAMES</NAME>
                </LOV>
                <LBL>表名</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <RELATED_FIELD>TABLE_NAME</RELATED_FIELD>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>TABLE_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_GET_COLUMNNAME</NAME>
                </LOV>
                <LBL>分行列名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <RELATED_FIELD>BRANCH_COL_NAME</RELATED_FIELD>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>BRANCH_COL_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_GET_COLUMNNAME</NAME>
                </LOV>
                <LBL>账户列名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <RELATED_FIELD>ACCOUNT_COL_NAME</RELATED_FIELD>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>ACCOUNT_COL_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>可选列表</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_LIST</BLOCK>
              <WIDTH>80</WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT>100</HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_3" COLLAPSE="N">
          <PART NAME="P3" ID="P3" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_STAT">
              <FIELD INDEX="" CONTROL="N">
                <LBL>表名</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>TABLE_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>分行列名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>BRANCH_COL_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>账户列名称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>120</MAXLENGTH>
                <NAME>ACCOUNT_COL_NAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>必填列表</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_STAT</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT>100</HEIGHT>
              <READ_ONLY>-1</READ_ONLY>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>