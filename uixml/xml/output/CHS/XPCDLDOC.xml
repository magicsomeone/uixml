                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <NAME>PRDCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <SIZE>12</SIZE>
                <COLS></COLS>
                <MAXLENGTH>12</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DCUMNETCD</NAME>
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_DOC</NAME>
                </LOV>
                <LBL>单据代码</LBL>
                <INPUT_LOV>-1</INPUT_LOV>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
