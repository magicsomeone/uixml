                <SIZE>30</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>255</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DOCUDESC</NAME>
                <ROWS></ROWS>
              </FIELD>
