<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="向 HO 出售旅行支票" TMP_SCR_TYPE="M">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL>全部</LBL>
        <SECTION ID="sec" COLLAPSE="N">
          <PART NAME="P1" ID="P1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FLD_SET1">
              <FIELD INDEX="" CONTROL="N">
                <LBL>外部参考编号</LBL>
                <SIZE>20</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>XREF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <OFFLINE_LOV>
                  <TYPE>G</TYPE>
                  <NAME>LOV_CURRENCY_LOCAL</NAME>
                </OFFLINE_LOV>
                <SIZE>3</SIZE>
                <COLS></COLS>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>TXNCCY</NAME>
                <CASE>UPPER</CASE>
                <LBL>交易币种</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <OFFLINE_LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_CLGBNK</NAME>
                </OFFLINE_LOV>
                <SIZE>3</SIZE>
                <COLS></COLS>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CLGBNK</NAME>
                <CASE>UPPER</CASE>
                <LBL>发卡行代码</LBL>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="P2" ID="P2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FLD_SET2">
              <FIELD INDEX="" CONTROL="N">
                <LBL>分行代码</LBL>
                <SIZE>3</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>BRN</NAME>
                <CASE>UPPER</CASE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE>22</SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <RELATED_FIELD>TXNCCY</RELATED_FIELD>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>TXNAMT</NAME>
                <LBL>交易金额</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>AMOUNT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC></OK_IMG_SRC>
                  <TITLE>备注</TITLE>
                  <CANCEL_IMG_SRC></CANCEL_IMG_SRC>
                </POPUPEDIT>
                <LBL>备注</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>255</MAXLENGTH>
                <NAME>NARRATIVE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_TRANSACTION_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_TCDENOM">
        <LBL>旅行支票面值</LBL>

      </TAB>
    </BODY>
    <CALLFORMS>
      <FORM SEQ="1" id="CTCD">
        <CLFR_TYPE>DATA</CLFR_TYPE>
        <DISP_TAB>TAB_TCDENOM</DISP_TAB>
        <DISP_TYPE>TAB</DISP_TYPE>
        <FUNCTION>CTCD</FUNCTION>
      </FORM>
    </CALLFORMS>
  </SCREEN>
</FORM>