              <FIELD INDEX="" CONTROL="N">
                <LBL>起始日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
