                <ROWS></ROWS>
              </FIELD>
              <LBL>起息日</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CCVWSREPORT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="P3" ID="P3" WIDTH="50">
