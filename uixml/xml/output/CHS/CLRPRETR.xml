                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_PROD_CODE</NAME>
                </LOV>
