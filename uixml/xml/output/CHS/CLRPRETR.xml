                  <TYPE>L</TYPE>
                  <NAME>LOV_PROD_TYPE</NAME>
                </LOV>
                <LBL>产品类型</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
