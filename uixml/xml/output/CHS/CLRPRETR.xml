                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PM_MATURITY_TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>账户状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="A" SELECTED="0">有效</OPTION>
                <OPTION VALUE="H" SELECTED="0">暂挂</OPTION>
                <OPTION VALUE="R" SELECTED="0">已冲正</OPTION>
                <OPTION VALUE="L" SELECTED="0">已清算</OPTION>
                <OPTION VALUE="Y" SELECTED="0">已发起</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PM_ACCOUNT_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>授权状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="A" SELECTED="0">已授权</OPTION>
                <OPTION VALUE="U" SELECTED="0">未授权</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PM_AUTH_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>合同检索</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CCVWSREPORT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
