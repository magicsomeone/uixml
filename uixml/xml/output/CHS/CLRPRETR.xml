                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>终止日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>PM_TO_MAT_DATE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>到期日</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CCVWSREPORT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
