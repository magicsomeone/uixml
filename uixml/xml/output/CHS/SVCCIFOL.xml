<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_MAIN" POSITION="template" TITLE="客户签字人详细信息" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL>全部</LBL>
        <SECTION ID="SEC1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_CUST">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>客户编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>9</MAXLENGTH>
                <NAME>CUSTNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>全称</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <NAME>FNAME</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>签名</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CUST</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <PART NAME="PART2" ID="PART2" WIDTH="50">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_IMG">
              <FIELD INDEX="" CONTROL="N">
                <LBL>分行</LBL>
                <SIZE>3</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>BRN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CUST</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC2" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_CIFSIG">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>签名 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>9</MAXLENGTH>
                <NAME>SIGID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>签名标题</LBL>
                <SIZE>50</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>105</MAXLENGTH>
                <NAME>SIGTITLE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>签名/图像</LBL>
                <SIZE></SIZE>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnDisplaySignature(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_SIGN</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="Y">
                <LBL>图像</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE></DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <EVENT>
                  <FUNCTION>fnDisplayImage(event)</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_IMAGE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>客户签字人详细信息</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_CIFSIG</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT>500</HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>