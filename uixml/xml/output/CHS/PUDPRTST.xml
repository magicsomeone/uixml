                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>RESP_CREATION_DTM</NAME>
                <ROWS></ROWS>
              </FIELD>
