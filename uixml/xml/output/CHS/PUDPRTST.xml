        <SCREEN>CVS_REQUEST</SCREEN>
        <LBL>请求</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
      <FORM TYPE="S" SEQ="2" id="CVS_RESPONSE">
        <SCREEN>CVS_RESPONSE</SCREEN>
        <LBL>响应</LBL>
        <FUNCTION></FUNCTION>
      </FORM>
    </SUBSCREEN>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_1" COLLAPSE="N">
          <PART NAME="PART1" ID="PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_MASTER">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>主机代码</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>8</MAXLENGTH>
                <NAME>HOST_CODE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>LBL_HOST_CODE1</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>8</MAXLENGTH>
                <NAME>HOST_CODE1</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>主机代码说明</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>HOST_DESCRIPTION</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>参与人 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>PARTICIPANT_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>LBL_PARTICIPANT_ID1</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>PARTICIPANT_ID1</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>状态</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_MASTER</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
