                <LBL>响应消息时间</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="" SELECTED="-1"></OPTION>
                <OPTION VALUE="A" SELECTED="0">LBL_SIGNED_ON1</OPTION>
                <OPTION VALUE="I" SELECTED="0">LBL_SIGNED_OFF1</OPTION>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>RESP_MSG_TYPE</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>响应接收日期和时间</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>7</MAXLENGTH>
                <NAME>RESP_RECEIVED_DTM</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>响应被指示参与人 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>RES_INSTDAGT_PARTICIPANT_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>响应指令参与人 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <NAME>RES_INSTGAGT_PARTICIPANT_ID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>状态</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_DETAIL</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>15</PGSIZE>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
