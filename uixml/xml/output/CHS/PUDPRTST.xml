                <LBL>响应消息 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>35</MAXLENGTH>
                <NAME>RESPONSE_MSGID</NAME>
