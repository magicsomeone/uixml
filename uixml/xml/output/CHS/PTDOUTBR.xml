      <BUTTON ID="1">
        <BUTTON_EVENT>fnViewMsg()</BUTTON_EVENT>
        <BUTTON_LBL>查看消息</BUTTON_LBL>
        <BUTTON_NAME>BTN_VW_MSG</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="2">
        <BUTTON_EVENT>fnViewAccounting()</BUTTON_EVENT>
        <BUTTON_LBL>查看会计</BUTTON_LBL>
        <BUTTON_NAME>BTN_VW_ACCOUNTING</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="3">
        <BUTTON_EVENT>fnViewTxn()</BUTTON_EVENT>
        <BUTTON_LBL>View Transaction</BUTTON_LBL>
        <BUTTON_NAME>BTN_VW_TXN</BUTTON_NAME>
      </BUTTON>
      <BUTTON ID="4">
