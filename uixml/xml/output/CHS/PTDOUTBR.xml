        <MAXLENGTH>22</MAXLENGTH>
        <MIN_VAL></MIN_VAL>
        <NAME>DISPATCH_REF_NO</NAME>
      </FIELD>
      <FIELD TYPE="ME" ID="2">
        <LBL>发送日期</LBL>
        <DTYPE>DATE</DTYPE>
        <TYPE>DATE</TYPE>
        <MAXLENGTH>11</MAXLENGTH>
