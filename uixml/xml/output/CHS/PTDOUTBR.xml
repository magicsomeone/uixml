      </FIELD>
      <FIELD TYPE="ME" ID="3">
        <LBL>批时间</LBL>
        <DTYPE>VARCHAR2</DTYPE>
        <TYPE>TEXT</TYPE>
        <MAXLENGTH>4</MAXLENGTH>
