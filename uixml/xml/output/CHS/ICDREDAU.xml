<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_AUTH" POSITION="template" TITLE="授权" TMP_SCR_TYPE="S">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL/>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL/>

      </TAB>
    </FOOTER>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL/>
        <SECTION ID="SEC_REDEMPTION" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_REDEMPTION_1">
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>赎回参考编号</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <NAME>REDEM_REF_NO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="1">
                <LBL>分行代码</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>BRANCH_CODE</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>定期存款账号</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>20</MAXLENGTH>
                <NAME>ACC_NO</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N" SPRT="2">
                <LBL>账户币种</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>3</MAXLENGTH>
                <NAME>CCY</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_REDEMPTION_MASSTER</BLOCK>
              <WIDTH/>
              <HEIGHT/>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL/>

        </SECTION>
        <SECTION ID="SEC_OVERRIDES" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_OVERRIDES">
              <FIELD INDEX="" CONTROL="N">
                <LBL>已确认</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>CONFIRMED</NAME>
                <ROWS/>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <POPUPEDIT>
                  <OK_IMG_SRC/>
                  <TITLE>授权文本</TITLE>
                  <CANCEL_IMG_SRC/>
                </POPUPEDIT>
                <LBL>授权文本</LBL>
                <SIZE/>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS/>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>255</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>OVDTXT</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_OVERRIDES</BLOCK>
              <WIDTH/>
              <PGSIZE>15</PGSIZE>
              <HEIGHT/>
            </FLDSET>

          </PART>
          <LBL/>

        </SECTION>
        <SECTION ID="SEC_BUTTON" COLLAPSE="N">
          <PART NAME="PART_1" ID="PART_1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_BUTTON">
              <FIELD INDEX="" CONTROL="Y">
                <LBL>授权</LBL>
                <SIZE/>
                <DTYPE/>
                <COLS/>
                <TYPE>BUTTON</TYPE>
                <MAXLENGTH/>
                <EVENT>
                  <FUNCTION>fnOnlineAuth()</FUNCTION>
                  <NAME>onclick</NAME>
                </EVENT>
                <NAME>BTN_AUTH</NAME>
                <ROWS/>
              </FIELD>
              <LBL/>
              <HREQ>0</HREQ>
              <BLOCK>BLK_REDEMPTION_MASSTER</BLOCK>
              <WIDTH/>
              <HEIGHT/>
            </FLDSET>

          </PART>
          <LBL/>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>