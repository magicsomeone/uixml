                <LBL>功能 ID</LBL>
                <SIZE>1</SIZE>
                <ID>REPREF</ID>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>REPREF</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="2" SPRT="1">
                <LBL>功能 ID</LBL>
                <SIZE>1</SIZE>
                <ID>REPFID</ID>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>REPFID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="3" SPRT="1">
