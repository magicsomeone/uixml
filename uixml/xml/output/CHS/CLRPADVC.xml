                <LBL>Report Format</LBL>
                <ID>REPFMT</ID>
                <DTYPE>CHAR</DTYPE>
                <TYPE>SELECT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD SPRT="1" INDEX="6">
