              <LBL>查看通知单</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_REP_CL_PADVC</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION NAME="SEC_REPORT_OPTIONS" ID="SEC_REPORT_OPTIONS">
