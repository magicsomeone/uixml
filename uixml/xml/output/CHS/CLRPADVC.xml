                <OPTION VALUE="C" SELECTED="0">客户端</OPTION>
                <OPTION VALUE="S">服务器</OPTION>
                <COLS></COLS>
                <CHECKED>N</CHECKED>
                <MAXLENGTH>1</MAXLENGTH>
                <NAME>PRINTAT</NAME>
                <LBL>Printer At</LBL>
                <ID>PRINTAT</ID>
                <DTYPE>CHAR</DTYPE>
                <TYPE>SELECT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD SPRT="2" INDEX="3">
                <SIZE>15</SIZE>
                <WIDTH>3</WIDTH>
                <COLS></COLS>
                <MAXLENGTH>15</MAXLENGTH>
                <NAME>PRINTER</NAME>
                <LOV>
                  <TYPE>G</TYPE>
                  <NAME>LOV_EXTRPT_PRINTER</NAME>
                </LOV>
                <LBL>Printer</LBL>
                <ID>PRINTER</ID>
                <DTYPE>VARCHAR2</DTYPE>
                <TYPE>TEXT</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD SPRT="2" INDEX="4">
                <SIZE>4</SIZE>
                <WIDTH>3</WIDTH>
                <HIDDEN>-1</HIDDEN>
                <COLS></COLS>
                <MAXLENGTH>32000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PARAMVALS</NAME>
                <LBL>Parameter Values</LBL>
                <ID>PARAMVALS</ID>
                <DTYPE>VARCHAR</DTYPE>
                <TYPE>HIDDEN</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <FIELD SPRT="2" INDEX="5">
                <SIZE>4</SIZE>
                <WIDTH>3</WIDTH>
                <COLS></COLS>
                <MAXLENGTH>32000</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>PARAMTYPES</NAME>
                <LBL>File Name</LBL>
                <ID>FILENAME</ID>
                <DTYPE>VARCHAR</DTYPE>
                <TYPE>HIDDEN</TYPE>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_REPORT_OPTIONS</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>
            <SPRTCNT>2</SPRTCNT>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
