<?xml version="1.0" encoding="UTF-8"?>
<FORM>
  <SCREEN MAIN_WIN="Y" NAME="CVS_EVENTS" POSITION="template" TITLE="主要" TMP_SCR_TYPE="L">
    <EXIT_BTN>1</EXIT_BTN>
    <HEADER>
      <TAB ID="TAB_HEADER">
        <LBL></LBL>

      </TAB>
    </HEADER>
    <FOOTER>
      <TAB ID="TAB_FOOTER">
        <LBL></LBL>

      </TAB>
    </FOOTER>
    <SUBSCREEN>
      <FORM TYPE="LF" SEQ="1" id="CSDACENT">
        <SCREEN>CVS_ACCOUNTS</SCREEN>
        <LBL>会计分录</LBL>
        <FUNCTION>CSDACENT</FUNCTION>
      </FORM>
      <FORM TYPE="LF" SEQ="2" id="CSDMSGVW">
        <SCREEN>CVS_MESSAGES</SCREEN>
        <LBL>消息</LBL>
        <FUNCTION>CSDMSGVW</FUNCTION>
      </FORM>
    </SUBSCREEN>
    <BODY>
      <TAB ID="TAB_MAIN">
        <LBL></LBL>
        <SECTION ID="SEC_EVENT1" COLLAPSE="N">
          <PART NAME="SEC_EVENT1_PART1" ID="SEC_EVENT1_PART1" WIDTH="100">
            <FLDSET VIEW="SE" TYPE="SE" INDEX="1" ID="FST_FCCREF">
              <FIELD INDEX="" CONTROL="N">
                <REQD>-1</REQD>
                <LBL>参考编号</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>16</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>REFNO</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD CONTROL="N" INDEX="">
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <HIDDEN>-1</HIDDEN>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <NAME>LATEVNSEQNO</NAME>
                <LBL>最新事件序号</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>HIDDEN</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL></LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>UICOL1</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>父函数 ID</LBL>
                <SIZE></SIZE>
                <HIDDEN>-1</HIDDEN>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>HIDDEN</TYPE>
                <MAXLENGTH>1000</MAXLENGTH>
                <NAME>PARENTFID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL></LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_EVENT</BLOCK>
              <WIDTH></WIDTH>
              <HEIGHT></HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
        <SECTION ID="SEC_EVENT2" COLLAPSE="N">
          <PART NAME="SEC_EVENT2_PART1" ID="SEC_EVENT2_PART1" WIDTH="100">
            <FLDSET VIEW="ME" TYPE="ME" INDEX="1" ID="FST_EVENT">
              <FIELD CONTROL="N" INDEX="">
                <REQD>-1</REQD>
                <MAX_DECIMALS></MAX_DECIMALS>
                <SIZE></SIZE>
                <MAX_VAL></MAX_VAL>
                <COLS></COLS>
                <MAXLENGTH>22</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>EVENTNO</NAME>
                <LBL>事件编号</LBL>
                <DTYPE>NUMBER</DTYPE>
                <TYPE>TEXT</TYPE>
                <MIN_VAL></MIN_VAL>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LOV>
                  <TYPE>L</TYPE>
                  <NAME>LOV_EVENT</NAME>
                </LOV>
                <LBL>事件代码</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>4</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>EVENTCD</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>说明</LBL>
                <SIZE>35</SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH></MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>DESCRIPTION</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>事件日期</LBL>
                <SIZE></SIZE>
                <DTYPE>DATE</DTYPE>
                <COLS></COLS>
                <TYPE>DATE</TYPE>
                <MAXLENGTH>11</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>EVENTDT</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>授权状态</LBL>
                <SIZE></SIZE>
                <OPTION VALUE="A" SELECTED="0">已授权</OPTION>
                <OPTION VALUE="U" SELECTED="0">未授权</OPTION>
                <DTYPE>CHAR</DTYPE>
                <COLS></COLS>
                <TYPE>SELECT</TYPE>
                <MAXLENGTH>1</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>AUTH_STATUS</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>经办者 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>MAKERID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <FIELD INDEX="" CONTROL="N">
                <LBL>复核者 ID</LBL>
                <SIZE></SIZE>
                <DTYPE>VARCHAR2</DTYPE>
                <COLS></COLS>
                <TYPE>TEXT</TYPE>
                <MAXLENGTH>12</MAXLENGTH>
                <READ_ONLY>-1</READ_ONLY>
                <NAME>CHECKERID</NAME>
                <ROWS></ROWS>
              </FIELD>
              <LBL>事件</LBL>
              <HREQ>0</HREQ>
              <BLOCK>BLK_EVENT_DETAILS</BLOCK>
              <WIDTH></WIDTH>
              <PGSIZE>20</PGSIZE>
              <HEIGHT>500</HEIGHT>
            </FLDSET>

          </PART>
          <LBL></LBL>

        </SECTION>
      </TAB>
    </BODY>
  </SCREEN>
</FORM>